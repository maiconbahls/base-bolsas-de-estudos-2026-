    'CURSO': ['CURSO', 'PROGRAMA', 'NOME_CURSO', 'CURSO_FORMACAO', 'DESC_CURSO'],
    'INSTITUICAO': ['INSTITUICAO', 'INSTITUIÃ‡ÃƒO', 'IES', 'FACULDADE', 'UNIVERSIDADE', 'NOME_INSTITUICAO', 'NM_INSTITUICAO', 'INSTITUIO'],
    'NIVEL': ['NIVEL', 'NÃVEL', 'GRAU', 'MODALIDADE', 'NIVEL_ENSINO', 'TIPO', 'DESC_NIVEL'],
